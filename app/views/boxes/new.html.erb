<div class="new-boxes-container">
  <h1 class="text color-black">Create your DogHouse </h1>
  <div class="form">
    <%= simple_form_for(@box) do |f| %>
      <%= f.input :name %>
      <%= f.input :description %>
      <%= f.input :address %>
      <%= f.input :price_per_day %>
    <%= f.button :submit %>
  <% end %>
  </div>

</div>
