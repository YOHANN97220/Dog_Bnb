
<a href="<%=box_path(box)%>" class= "btn">
  <div class="container-popup">
    <div class="popup">
      <img src="<%= box.image_url %>" alt=" image">


    </div>
    <div class="title">
      <h3><%= box.name %></h3>
    </div>

    <div class="text" type="d-flex">
      <p><%= box.address %></p>
    </div>
  </div>
</a>

