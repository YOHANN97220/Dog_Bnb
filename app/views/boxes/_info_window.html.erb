<a  class="container-popup text-decoration-none" href="<%=box_path(box)%>" class= "btn">
  <div class="popup">

    <div class="popup mt-2 ">
      <img src="<%= box.image_url %>" alt=" image">
    </div>

    <div class="title mt-3">
      <h3><%= box.name %></h3>
    </div>

    <div class="text" type="d-flex">
      <p><%= box.address %></p>
    </div>

  </div>
</a>

